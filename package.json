{
  "name": "angular2-prod-ready",
  "version": "0.0.1",
  "description": "Angular2 template for a production-ready application",
  "main": "index.js",
  "dependencies": {
    "angular2": "^2.0.0-beta.12",
    "autoprefixer": "^6.3.5",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.4",
    "systemjs": "^0.19.25",
    "zone.js": "^0.6.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-inline-ng2-styles": "0.0.1",
    "gulp-inline-ng2-template": "^1.1.3",
    "gulp-less": "^3.0.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.12.1",
    "gulp-watch": "^4.3.5",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
